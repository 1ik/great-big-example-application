{
  "name": "great-big-angular2-example",
  "version": "0.0.0",
  "engines": {
    "node": "7.5.0"
  },
  "description": "A fullstack, multi-feature Angular2/Redux app made from several single-feature apps",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "cover": "istanbul report --include=**/chrome/coverage-final.json text text-summary",
    "debug-server": "cross-env NODE_ENV=development nodemon --nolazy --debug-brk=5858 server/src",
    "e2e": "protractor",
    "xbackend": "json-server --watch server/db/db.json --port 3001",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cross-env NODE_ENV=development ng serve --proxy-config proxy.conf.json",
    "dev:server": "cross-env NODE_ENV=development nodemon server/src",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "ng": "ng",
    "prebuild": "rimraf dist/",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "pretest": "rimraf coverage/ & npm run lint",
    "posttest": "npm run cover",
    "prod": "concurrently -r \"npm run prod:client\" \"npm run prod:server\"",
    "prod:client": "ng serve --proxy-config proxy.conf.json",
    "prod:server": "cross-env NODE_ENV=production node server/src",
    "prod:server:heroku": "cross-env NODE_ENV=production node server/src",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "start": "concurrently -r \"npm run prod:client\" \"npm run prod:server:heroku\"",
    "test": "ng test",
    "test:watch": "karma start --singleRun false",
    "postinstall": "ng build --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dancancro/great-big-angular2-example.git"
  },
  "keywords": [
    "feathers",
    "feathersjs",
    "hook",
    "hooks",
    "angular2",
    "redux",
    "starter",
    "boilerplate",
    "login"
  ],
  "private": true,
  "bugs": {
    "url": "https://github.com/dancancro/great-big-angular2-example/issues"
  },
  "homepage": "https://github.com/dancancro/great-big-angular2-example#readme",
